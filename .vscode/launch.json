{
  "version": "0.2.0",
  "configurations": [
  {
    "type": "node",
    "request": "launch",
    "name": "Typescript Debug",
    "program": "${workspaceFolder}/src/main.ts",
    "preLaunchTask": "tsc: build - tsconfig.json",
    "outFiles": [
      "${workspaceFolder}/build/**/*.js",
      "!**/node_modules/**"
    ],
    "skipFiles": [
      "<node_internals>/**"
    ],
  }
  ],
  
}